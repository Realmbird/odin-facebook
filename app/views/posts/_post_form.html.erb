<%= stylesheet_link_tag 'post_form.css' %>
<%= stylesheet_link_tag 'post.css' %>
<div>
  <p>User Id: <%= post.user_id%> </p>
  <h3>Title: <%= post.title %> </h3>
  <p> <%= post.body %></p>
</div>
<%= post.class.name %>
<%= button_to "Like", likes_path(likable_id: post, likable_type: post.class.name), method: :post %>
<%= Like.where(likable_id: post).count %>